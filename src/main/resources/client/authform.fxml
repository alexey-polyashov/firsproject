<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="authPane" prefHeight="210.0" prefWidth="316.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.AuthWndController">
   <children>
      <Button fx:id="regNewUserBtn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" text="Register new user" />
      <Label layoutX="14.0" layoutY="61.0" prefHeight="18.0" prefWidth="85.0" text="Login" />
      <TextField fx:id="login" layoutX="96.0" layoutY="57.0" prefHeight="26.0" prefWidth="203.0" />
      <Label layoutX="14.0" layoutY="93.0" text="Password" />
      <PasswordField fx:id="password" layoutX="96.0" layoutY="88.0" prefHeight="26.0" prefWidth="203.0" />
      <Button fx:id="loginBtn" layoutX="96.0" layoutY="122.0" mnemonicParsing="false" onAction="#doLogin" text="Login" />
      <Button fx:id="remindPasswordBtn" layoutX="14.0" layoutY="169.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="110.0" text="Remind password" />
   </children>
</AnchorPane>
